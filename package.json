{
  "name": "@chayns/swipeable",
  "version": "0.1.0",
  "description": "A wrapper component that reveals contextual actions on swipe.",
  "keywords": [
    "chayns",
    "chayns-components",
    "swipe",
    "react",
    "mobile"
  ],
  "repository": "github:tobitsoftware/chayns-swipeable",
  "license": "MIT",
  "author": "Leo Driesch <leonhard.driesch@tobit.com> (labs.tobit.com)",
  "main": "./dist/index.js",
  "unpkg": "./dist/index.umd.js",
  "module": "./dist/index.esm.js",
  "source": "src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "microbundle --jsx React.createElement --no-compress",
    "format": "prettier . --write --ignore-path .gitignore",
    "prepublishOnly": "rimraf dist/ && npm run build",
    "watch": "microbundle watch --jsx React.createElement --no-compress",
    "check-types": "tsc",
    "lint": "eslint src/"
  },
  "devDependencies": {
    "@types/react": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.27.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "microbundle": "^0.13.1",
    "prettier": "^2.3.0",
    "prettier-plugin-packagejson": "^2.2.11",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2"
  },
  "peerDependencies": {
    "framer-motion": ">=4",
    "react": ">=16.8.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
